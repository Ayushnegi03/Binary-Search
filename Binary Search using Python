//Binary Search
def BS(arr,a,l,h):
    while l <= h:
        mid = l + (h-l)//2
        if arr[mid] == a:
            return mid
        elif arr[mid] < a:
            l=mid+1
        else:
            h=mid-1
    return -1
arr = [23,24,25,26,27,28,29]
a = 24
result = BS(arr, a, 0, len(arr)-1)
if result != -1:
    print("Elements is shown in index\n" + str(result))
else:
    print("Not Found")
#Output
#1
